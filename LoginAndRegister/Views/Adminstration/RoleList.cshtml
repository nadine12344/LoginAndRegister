@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>


<h1>Roles</h1>
@if (Model.Any())
{

    foreach (var role in Model)
    {
        <div value="role">@role.Name</div>
    }
    <select  class="form-control" asp-items='new SelectList(Model,"Id","Name")'>
    </select>
}
else
{
    <h3>No Roles yet</h3>
}